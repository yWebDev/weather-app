{
  "extends": "eslint:recommended",
  "env": {
    "browser": true,
    "node": true
  },
  "ecmaFeatures": {
  },
  "rules": {
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1,
        "VariableDeclarator": 1
      }
    ],
    "comma-dangle": [
      1,
      "never"
    ],
    "func-names": 0,
    "key-spacing": 0,
    "curly": [
      2,
      "all"
    ],
    "no-var": 0,
    "vars-on-top": 2,
    "no-else-return": 0,
    "no-multi-spaces": 0,
    "quotes": [
      2,
      "single"
    ],
    "no-console": 2,
    "no-param-reassign": 0,
    "prefer-template": 0,
    "object-shorthand": 0,
    "max-len": [
      1,
      120,
      4
    ],
    "wrap-iife": [
      2,
      "inside"
    ],
    "spaced-comment": 0,
    "no-shadow": 0,
    "no-use-before-define": [
      2,
      "nofunc"
    ],
    "prefer-arrow-callback": 0
  },
  "globals": {
    "define": true
  }
}
